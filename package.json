{
    "name": "minimal-polyfills",
    "version": "2.1.1",
    "main": "./dist/index.js",
    "types": "./src/index.ts",
    "description": "An ultra light-weight, nonexhaustive, polyfills library",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/garronej/minimal_polyfills.git"
    },
    "scripts": {
        "test:node": "node ./dist/test",
        "test:deno": "deno run ./deno_dist/test/index.ts",
        "test": "npm run test:node && npm run test:deno",
        "tsc": "npx tsc",
        "denoify": "npx denoify",
        "build": "npm run tsc && npm run denoify",
        "enable_short_import_path:npm": "npm run build && npx denoify_enable_short_npm_import_path",
        "enable_short_import_path:deno": "npm run build && npx denoify_enable_short_deno_import_path",
        "clean": "rm -rf dist deno_dist node_modules mod.ts"
    },
    "author": "Joseph Garrone",
    "license": "MIT",
    "keywords": [
        "polyfills",
        "light-weight",
        "nonexhaustive",
        "Map",
        "Set",
        "WeakMap",
        "typescript",
        "Array.prototype.find",
        "Array.prototype.from",
        "Array.prototype.isView",
        "String.prototype.startsWith"
    ],
    "dependencies": {},
    "devDependencies": {
        "typescript": "^3.9.3",
        "denoify": "^0.2.15"
    },
    "files": [
        "/src/*.ts",
        "/dist/*.{d.ts,js,js.map}"
    ]
}
